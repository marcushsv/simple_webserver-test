<!DOCTYPE html>
<html>
    <head>
        <title>Benutzerliste</title>
    
    </head>
    <body>
        <h1>Liste aller Benutzer:</h1>
        <table border="1">
            <tr>
                <th>ID</th>
                <th>Benutzer</th>
                <th>Passwort</th>
            </tr>
            <% for(var i = 0; i < user.length; i++){ %>
                <tr>
                    <td><%= user[i].id %></td>
                    <td><%= user[i].benutzername %></td>
                    <td><%= user[i].passwort %></td>
                </tr>
            <%  } %>
            
        </table>
</body>

</html>